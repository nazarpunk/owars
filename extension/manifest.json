{
  "manifest_version": 2,
  "name": "owars",
  "version": "1.0.0",
  "description": "Кастом для owars.ru",
  "homepage_url": "http://gorsaldo.xyz",
  "update_url": "https://github.com/nazarpunk/owars/blob/master/updates.xml",
  "minimum_chrome_version": "83",
  "icons": {
    "16": "img/ico/ext-16.png",
    "32": "img/ico/ext-32.png",
    "48": "img/ico/ext-48.png",
    "64": "img/ico/ext-64.png",
    "128": "img/ico/ext-128.png"
  },
  "background": {
    "scripts": [
      "js/background/background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "*://*.owars.ru/*"
      ],
      "js": [
        "js/content/content.js"
      ],
      "all_frames": true
    }
  ],
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://*.owars.ru/*"
  ],
  "web_accessible_resources": [
    "page/*"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.owars.ru/*"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "img/ico/ext-16.png",
      "32": "img/ico/ext-32.png",
      "48": "img/ico/ext-48.png",
      "64": "img/ico/ext-64.png",
      "128": "img/ico/ext-128.png"
    },
    "default_title": "owars"
  }
}